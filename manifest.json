{
    "manifest_version": 3,
   
    "name": "Modify CSS and HTML for V-Klass",
    "version": "0.1.0",
    "description": "Makes V-Klass prittier and adds darkmode",
   
    "background": {
        "service_worker": "script.js"
        
        // "persistent": false
    },

    "permissions": [
        "tabs",
        "storage",
        "scripting"
    ],

    "host_permissions": [
        "https://www.vklass.se/"
    ],

    "action": {
        "default_popup": "popup.html"
    }

